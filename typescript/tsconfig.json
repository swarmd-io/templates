{
  "compilerOptions": {
    "noEmit": true,
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": false,
    "lib": [
      "ESNext",
      "WebWorker",
      "DOM"
    ],
    "sourceMap": false,
    "declaration": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "types": [],
    "plugins": []
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "./node_modules"
  ]
}
